	<parameter name="pedidoId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT 
    ip.id AS item_pedido_id,
    ip.produto_id,
    ip.pedido_id,
    ip.qtde,
    ip.valor_item AS valor_pago,
    p.descricao AS produto_descricao,
    p.valor_unitario AS valor_item_atual
FROM 
    public.item_pedido ip
JOIN 
    public.produto p ON ip.produto_id = p.id
WHERE 
    ip.pedido_id = $P!{pedidoId}
ORDER BY 
    ip.id]]>
	</queryString>
